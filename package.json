    {
      "path": "dist/antd-theme-vars.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "husky": "^8.0.1",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^5.0.0",
    "@types/fs-extra": "^9.0.13",
    "fs-extra": "^10.1.0",
    "less-vars-to-js": "^1.3.0"
  }
